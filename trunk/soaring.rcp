FORM ID form_set_sms AT (0 0 160 160)
MODAL
NOSAVEBEHIND
MENUID menu_main
BEGIN
  NOGRAFFITISTATEINDICATOR
  TITLE "Send SMS Settings"
  CHECKBOX "Send SMS Messages" ID form_set_sms_enable AT (1 15 AUTO AUTO) FONT 1
  LABEL "Address:" ID form_set_sms_addresslbl AT (PREVLEFT PREVBOTTOM+3) FONT 1
  FIELD ID form_set_sms_address AT (PREVLEFT+10 PREVBOTTOM 145 30) FONT 0 UNDERLINED MULTIPLELINES MAXCHARS 40 
  LABEL "Output Type:" ID form_set_sms_outlbl AT (1 PREVBOTTOM) FONT 1
  CHECKBOX "Generic" ID form_set_sms_outgen AT (PREVLEFT PREVBOTTOM AUTO AUTO) FONT 0 GROUP 1
  CHECKBOX "Pathaway" ID form_set_sms_outpw AT (PREVRIGHT+3 PREVTOP AUTO AUTO) FONT 0 GROUP 1
  LABEL "Messages To Send:" ID form_set_sms_sendlbl AT (1 PREVBOTTOM) FONT 1
  CHECKBOX "Taskoff" ID form_set_sms_sendto AT (PREVLEFT PREVBOTTOM AUTO AUTO) FONT 0 
  CHECKBOX "Landing" ID form_set_sms_sendland AT (PREVRIGHT+3 PREVTOP AUTO AUTO) FONT 0 
  CHECKBOX "Periodic" ID form_set_sms_sendperiod AT (PREVRIGHT+3 PREVTOP AUTO AUTO) FONT 0 
  LABEL "Interval:" ID form_set_sms_sendintlbl AT (1 PREVBOTTOM) FONT 1
  FIELD ID form_set_sms_sendint AT (PREVRIGHT+2 PREVTOP 25 AUTO) FONT 0 UNDERLINED MAXCHARS 4 RIGHTALIGN
  LABEL "mins." ID form_set_sms_minslbl AT (PREVRIGHT+3 PREVTOP) FONT 1
  BUTTON "Send Now" ID form_set_sms_sendnow AT (CENTER@80 PREVBOTTOM+3 55 AUTO) FONT 7 
END

FORM ID form_set_mc AT (2 86 156 72)
